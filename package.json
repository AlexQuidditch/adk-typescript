{
  "name": "@pontus-devoteam/adk",
  "version": "0.0.1",
  "description": "Agent Development Kit for TypeScript with multi-provider LLM support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "example:simple": "ts-node examples/simple-agent/index.ts",
    "example:multi": "ts-node examples/multi-provider/index.ts",
    "example:tool": "ts-node examples/tool-usage/index.ts",
    "example:memory": "ts-node examples/memory-usage/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pontus-devoteam/adk-typescript.git"
  },
  "keywords": [
    "ai",
    "llm",
    "agent",
    "openai",
    "anthropic",
    "gemini",
    "typescript"
  ],
  "author": "Pontus Espe",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.8.0",
    "@google-cloud/vertexai": "^0.5.0",
    "@google/generative-ai": "^0.1.3",
    "axios": "^1.6.2",
    "dotenv": "^16.4.7",
    "openai": "^4.93.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.17.30",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
